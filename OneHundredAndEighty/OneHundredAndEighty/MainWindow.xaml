<Window x:Class="OneHundredAndEighty.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OneHundredAndEighty"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Height="800"
        Width="1200"
        Title="OneHundredAndEighty"
        >
    <Window.Resources>
        <Style x:Key="CommonSegment">
            <EventSetter Event="Shape.MouseEnter" Handler="PointsShow"/>
            
            <Setter Property="Shape.Stroke" Value="Black"/>
            <Setter Property="Shape.StrokeThickness" Value="0.2"/>
            <Setter Property="Shape.Cursor" Value="Hand"/>
            <Setter Property="Shape.Opacity" Value="1"/>
            <Setter Property="Shape.Fill">
                <Setter.Value>
                    <SolidColorBrush Color="Transparent"/>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="DarkSegment" BasedOn="{StaticResource CommonSegment}">
            <Style.Triggers>
                <Trigger Property="Shape.IsMouseOver" Value="True">
                    <Trigger.EnterActions>
                        <BeginStoryboard Name="Blink">
                            <Storyboard>
                                <ColorAnimation Storyboard.TargetProperty="Fill.Color"
                                                To="Aqua"
                                                Duration="0:0:0.0">
                                </ColorAnimation>
                                <DoubleAnimation Storyboard.TargetProperty="Opacity"
                                                 From="0"
                                                 To="0.55"
                                                 Duration="0:0:0.20"
                                                 AutoReverse="True"
                                                 RepeatBehavior="Forever"
                                                 >
                                </DoubleAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.EnterActions>
                    <Trigger.ExitActions>
                        <StopStoryboard BeginStoryboardName="Blink" />
                        <BeginStoryboard>
                            <Storyboard>
                                <ColorAnimation Storyboard.TargetProperty="Fill.Color"
                                                To="Transparent"
                                                Duration="0:0:0.0">
                                </ColorAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.ExitActions>
                </Trigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="LightSegment" BasedOn="{StaticResource CommonSegment}">
            <Style.Triggers>
                <Trigger Property="Shape.IsMouseOver" Value="True">

                    <Trigger.EnterActions>
                        <BeginStoryboard Name="Blink">
                            <Storyboard>
                                <ColorAnimation Storyboard.TargetProperty="Fill.Color"
                                                To="DarkOrchid"
                                                Duration="0:0:0.0">
                                </ColorAnimation>
                                <DoubleAnimation Storyboard.TargetProperty="Opacity"
                                                 From="0"
                                                 To="0.55"
                                                 Duration="0:0:0.20"
                                                 AutoReverse="True"
                                                 RepeatBehavior="Forever"
                                                 >
                                </DoubleAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.EnterActions>
                    <Trigger.ExitActions>
                        <StopStoryboard BeginStoryboardName="Blink" />
                        <BeginStoryboard>
                            <Storyboard>
                                <ColorAnimation Storyboard.TargetProperty="Fill.Color"
                                                To="Transparent"
                                                Duration="0:0:0.0">
                                </ColorAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.ExitActions>
                </Trigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="RedSegment" BasedOn="{StaticResource CommonSegment}">
            <Style.Triggers>
                <Trigger Property="Shape.IsMouseOver" Value="True">

                    <Trigger.EnterActions>
                        <BeginStoryboard Name="Blink">
                            <Storyboard>
                                <ColorAnimation Storyboard.TargetProperty="Fill.Color"
                                                To="LawnGreen"
                                                Duration="0:0:0.0">
                                </ColorAnimation>
                                <DoubleAnimation Storyboard.TargetProperty="Opacity"
                                                 From="0"
                                                 To="0.55"
                                                 Duration="0:0:0.20"
                                                 AutoReverse="True"
                                                 RepeatBehavior="Forever"
                                                 >
                                </DoubleAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.EnterActions>
                    <Trigger.ExitActions>
                        <StopStoryboard BeginStoryboardName="Blink" />
                        <BeginStoryboard>
                            <Storyboard>
                                <ColorAnimation Storyboard.TargetProperty="Fill.Color"
                                                To="Transparent"
                                                Duration="0:0:0.0">
                                </ColorAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.ExitActions>
                </Trigger>
            </Style.Triggers>
        </Style>
        <Style x:Key="GreenSegment" BasedOn="{StaticResource CommonSegment}">
            <Style.Triggers>
                <Trigger Property="Shape.IsMouseOver" Value="True">

                    <Trigger.EnterActions>
                        <BeginStoryboard Name="Blink">
                            <Storyboard>
                                <ColorAnimation Storyboard.TargetProperty="Fill.Color"
                                                To="Salmon"
                                                Duration="0:0:0.0">
                                </ColorAnimation>
                                <DoubleAnimation Storyboard.TargetProperty="Opacity"
                                                 From="0"
                                                 To="0.55"
                                                 Duration="0:0:0.20"
                                                 AutoReverse="True"
                                                 RepeatBehavior="Forever"
                                                 >
                                </DoubleAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.EnterActions>
                    <Trigger.ExitActions>
                        <StopStoryboard BeginStoryboardName="Blink" />
                        <BeginStoryboard>
                            <Storyboard>
                                <ColorAnimation Storyboard.TargetProperty="Fill.Color"
                                                To="Transparent"
                                                Duration="0:0:0.0">
                                </ColorAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.ExitActions>
                </Trigger>
            </Style.Triggers>
        </Style>
    </Window.Resources>
