<Window x:Class="OneHundredAndEighty.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OneHundredAndEighty"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Height="856"
        Width="1200"
        Title="OneHundredAndEighty"
        Closing="MainWindow_Closing"
        Foreground="#FF1F1F1F"
        >
    <Window.Resources>
        <Style x:Key="CommonSegment">
            <EventSetter Event="Shape.MouseEnter" Handler="PointsShow"/>
            <EventSetter Event="Shape.MouseLeftButtonDown" Handler="Throw"/>

            <Setter Property="Shape.Stroke" Value="Transparent"/>
            <Setter Property="Shape.StrokeThickness" Value="0"/>
            <Setter Property="Shape.Cursor" Value="Hand"/>
            <Setter Property="Shape.Opacity" Value="1"/>
            <Setter Property="Shape.Fill" Value="Transparent"/>
            <Style.Triggers>
                <Trigger Property="Shape.IsMouseOver" Value="True">
                    <Trigger.EnterActions>
                        <BeginStoryboard Name="Blink">
                            <Storyboard>
                                <ColorAnimation Storyboard.TargetProperty="Fill.Color"
                                                To="Aqua"
                                                Duration="0:0:0.0">
                                </ColorAnimation>
                                <DoubleAnimation Storyboard.TargetProperty="Opacity"
                                                 From="0"
                                                 To="0.5"
                                                 Duration="0:0:0.25"
                                                 AutoReverse="True"
                                                 RepeatBehavior="Forever"
                                                 >
                                </DoubleAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.EnterActions>
                    <Trigger.ExitActions>
                        <StopStoryboard BeginStoryboardName="Blink" />
                        <BeginStoryboard>
                            <Storyboard>
                                <ColorAnimation Storyboard.TargetProperty="Fill.Color"
                                                To="Transparent"
                                                Duration="0:0:0.0">
                                </ColorAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </Trigger.ExitActions>
                </Trigger>
                <!--<EventTrigger RoutedEvent="Shape.MouseLeftButtonDown">
                    <EventTrigger.Actions>
                        <BeginStoryboard>
                            <Storyboard>
                                <ColorAnimation Storyboard.TargetProperty="Fill.Color"
                                                To="Red"
                                                Duration="0:0:0.15"
                                                AutoReverse="True">
                                </ColorAnimation>
                                <ColorAnimation Storyboard.TargetProperty="Fill.Color"
                                                To="Transparent"
                                                Duration="0:0:0.15">
                                </ColorAnimation>
                            </Storyboard>
                        </BeginStoryboard>
                    </EventTrigger.Actions>
                </EventTrigger>-->
            </Style.Triggers>
        </Style>
        <!--<Style x:Key="DarkSegment"
               BasedOn="{StaticResource CommonSegment}">
        </Style>
        <Style x:Key="LightSegment"
               BasedOn="{StaticResource CommonSegment}">
        </Style>
        <Style x:Key="RedSegment"
               BasedOn="{StaticResource CommonSegment}">
        </Style>
        <Style x:Key="GreenSegment"
               BasedOn="{StaticResource CommonSegment}">
        </Style>-->
    </Window.Resources>
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <LinearGradientBrush.RelativeTransform>
                <TransformGroup>
                    <ScaleTransform CenterY="0.5" CenterX="0.5"/>
                    <SkewTransform CenterY="0.5" CenterX="0.5"/>
                    <RotateTransform CenterY="0.5" CenterX="0.5"/>
                    <TranslateTransform/>
                </TransformGroup>
            </LinearGradientBrush.RelativeTransform>
            <GradientStop Color="#FF191919" Offset="0"/>
            <GradientStop Color="#FF939393" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>
    <TabControl x:Name="Tab"
        Canvas.Left="60"
        Canvas.Top="20"
        Background="{x:Null}"
        BorderBrush="{x:Null}"
        Foreground="{x:Null}" 
        FontFamily="Agency FB"
        FontWeight="Bold" 
        FontSize="18"
        SelectedIndex="0"
        Cursor="Hand"
        >
        <TabControl.Resources>
            <Style TargetType="TabItem">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="TabItem">
                            <Border
                                    Name="Border"
                                    BorderThickness="1,0,1,1"
                                    BorderBrush="Black"
                                    Margin="-1,0"
                                    Background="#FF111111"
                                    >
                                <ContentPresenter
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        ContentSource="Header"
                                        Margin="10,4"
                                    />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsSelected" Value="True">
                                    <Setter Property="Foreground" Value="#FF15EC00" />
                                    <Setter TargetName="Border" Property="Background" Value="#FF032E03" />
                                    <Setter TargetName="Border" Property="Margin" Value="0,0,0,-6" />
                                </Trigger>
                                <Trigger Property="IsSelected" Value="False">
                                    <Setter Property="Foreground" Value="#FF194400" />
                                </Trigger>
                                <Trigger Property="IsEnabled" Value="False">
                                    <Setter Property="Foreground" Value="#FF3E3E3E" />
                                    <Setter Property="Visibility" Value="Hidden"/>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </TabControl.Resources>
