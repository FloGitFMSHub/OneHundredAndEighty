                                         Grid.Column="3"
                                         Width="150"
                                         VerticalContentAlignment="Center" />
                                <Label Content="Nickname"
                                       Grid.Row="3"
                                       Grid.Column="1"
                                       HorizontalAlignment="Right"
                                       VerticalAlignment="Center" />
                                <TextBox Name="NewPlayerNickNameTextBox"
                                         Grid.Row="3"
                                         Grid.Column="3"
                                         Width="150"
                                         VerticalContentAlignment="Center" />
