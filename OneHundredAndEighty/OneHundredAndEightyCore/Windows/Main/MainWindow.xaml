                                         VerticalContentAlignment="Center" />
                                <Label Content="Nickname" HorizontalAlignment="Left" Margin="28,57,0,0"
                                       VerticalAlignment="Top" Height="27" Width="66" VerticalContentAlignment="Center"
                                       HorizontalContentAlignment="Right" />
