                    <Label Name="NewGamePlayer2Label"
                           Content="Player"
                           Grid.Row="5"
                           Grid.Column="9"
                           Visibility="Hidden"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Left" />
                    <ComboBox Name="NewGameSetsComboBox"
                              Grid.Row="3"
                              Grid.Column="7"
                              Visibility="Hidden"
                              SelectedIndex="2"
                              Width="60">
                        <ComboBoxItem>1</ComboBoxItem>
                        <ComboBoxItem>3</ComboBoxItem>
                        <ComboBoxItem>5</ComboBoxItem>
                        <ComboBoxItem>7</ComboBoxItem>
                        <ComboBoxItem>9</ComboBoxItem>
                    </ComboBox>
                    <ComboBox Name="NewGameLegsComboBox"
                              Grid.Row="3"
                              Grid.Column="9"
                              Visibility="Hidden"
                              SelectedIndex="1"
                              Width="60">
                        <ComboBoxItem>1</ComboBoxItem>
                        <ComboBoxItem>3</ComboBoxItem>
                        <ComboBoxItem>5</ComboBoxItem>
                        <ComboBoxItem>7</ComboBoxItem>
                        <ComboBoxItem>9</ComboBoxItem>
                    </ComboBox>
                    <Label Name="NewGameSetsLabel"
                           Content="Sets"
                           Grid.Row="1"
                           Grid.Column="7"
                           Visibility="Hidden" VerticalAlignment="Bottom" />
                    <Label Name="NewGameLegsLabel"
                           Content="Legs"
                           Grid.Row="1"
                           Grid.Column="9"
                           Visibility="Hidden" VerticalAlignment="Bottom" />
