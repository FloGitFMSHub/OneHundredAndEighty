                    <Label Content="Game type"
                           Grid.Row="3"
                           Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" />
                    <ComboBox Name="NewGameTypeComboBox"
                              Grid.Row="3"
                              Grid.Column="3"
                              SelectedIndex="0"
                              SelectionChanged="NewGameTypeComboBox_OnSelectionChanged"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Center"
                              Width="150"
                              Height="25">
                        <ComboBoxItem>FreeThrowsSingle</ComboBoxItem>
                        <ComboBoxItem>FreeThrowsDouble</ComboBoxItem>
                        <ComboBoxItem>Classic</ComboBoxItem>
                    </ComboBox>
                    <Label Content="Player"
                           Grid.Row="5"
                           Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" />
                    <ComboBox Name="NewGamePlayer1ComboBox"
                              Grid.Row="5"
                              Grid.Column="3"
                              ItemsSource="{Binding Path=Players}"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Center"
                              Width="150"
                              Height="25" />
                    <Button Name="StartGameButton"
                            Content="Start Game"
                            Grid.Row="7"
                            Grid.Column="3"
                            Click="StartGameButtonClick"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Width="150"
                            Height="25" />
                    <Button Name="StopGameButton"
                            Content="Stop Game"
                            Grid.Row="9"
                            Grid.Column="3"
                            Click="StopGameButtonClick"
                            IsEnabled="False"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Width="150"
                            Height="25" />
