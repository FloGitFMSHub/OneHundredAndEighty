<Window x:Class="OneHundredAndEightyCore.Windows.Main.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:resx="clr-namespace:OneHundredAndEightyCore.Resources"
        xmlns:main="clr-namespace:OneHundredAndEightyCore.Windows.Main"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        Height="638"
        Width="1197"
        Title="OneHundredAndEighty"
        ResizeMode="CanResizeWithGrip"
        Closing="OnClosing">
    <Window.DataContext>
        <main:MainWindowViewModel />
    </Window.DataContext>
    <Window.Resources>
        <Style x:Key="TextBoxStyle">
            <Setter Property="Control.VerticalContentAlignment" Value="Center" />
            <Setter Property="Control.Width" Value="50" />
            <Setter Property="Control.HorizontalAlignment" Value="Left" />
            <EventSetter Event="TextBox.TextChanged" Handler="SetSettingDirty" />
        </Style>
        <Style x:Key="SetupSliderStyle">
            <Setter Property="Control.HorizontalAlignment" Value="Center" />
            <Setter Property="Slider.Orientation" Value="Vertical" />
            <Setter Property="Slider.Maximum" Value="1000" />
            <Setter Property="Control.IsEnabled" Value="False" />
            <EventSetter Event="TextBox.TextChanged" Handler="SetSettingDirty" />
        </Style>
        <Style x:Key="LabelStyle">
            <Setter Property="Control.HorizontalAlignment" Value="Left" />
            <Setter Property="Control.VerticalAlignment" Value="Center" />
        </Style>
        <Style x:Key="SetupSectorsComboboxStyle" TargetType="ComboBox">
            <Setter Property="ItemsSource">
                <Setter.Value>
                    <x:Array Type="sys:String">
                        <sys:String>11</sys:String>
                        <sys:String>11/14</sys:String>
                        <sys:String>14</sys:String>
                        <sys:String>14/9</sys:String>
