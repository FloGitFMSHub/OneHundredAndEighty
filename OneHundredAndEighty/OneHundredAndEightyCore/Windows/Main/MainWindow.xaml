                            IsEnabled="False"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Width="150"
                            Height="25" />
                    <ComboBox x:Name="NewGamePlayer2ComboBox"
                              Grid.Row="5"
                              Grid.Column="5"
                              Grid.ColumnSpan="3"
                              ItemsSource="{Binding Players}"
